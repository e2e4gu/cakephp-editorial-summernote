{
    "name": "mindforce/cakephp-editorial-summernote",
    "description": "Editorial Summernote editor plugin for CakePHP",
	"homepage": "http://mindforce.me",
	"keywords": ["cakephp", "ui", "editor", "Summernote"],
	"type": "cakephp-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Denys Kyselov",
			"homepage": "https://mindforce.me"
		}
	],
	"support": {
		"issues": "https://github.com/mindforce/cakephp-editorial-summernote/issues",
		"source": "https://github.com/mindforce/cakephp-editorial-summernote"
	},
	"minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "~4.0",
        "mindforce/component-installer": "*",
        "mindforce/cakephp-editorial": "dev-cakephp4",
        "components/jquery": "*",
        "components/jqueryui": "*",
        "studio-42/elfinder": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "suggest": {
        "mindforce/cakephp-garderobe": "Allow to create user-friendly interfaces in fast and simple style",
        "mindforce/cakephp-editorial-codemirror": "Well known editor for editing pure html code"
    },
    "autoload": {
        "psr-4": {
            "Editorial\\Summernote\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Editorial\\Summernote\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
