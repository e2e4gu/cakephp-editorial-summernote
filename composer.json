{
    "name": "mindforce/cakephp-editorial-summernote",
    "description": "Editorial Summernote editor plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "3.0.x-dev",
        "mindforce/cakephp-editorial": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "suggest": {
        "mindforce/garderobe": "Allow to create user-friendly interfaces in fast and simple style"
    },
    "autoload": {
        "psr-4": {
            "Editorial\\Summernote\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Editorial\\Summernote\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
